{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "build:pre": "nuxt cleanup",
    "build:app": "nuxt build",
    "dev:pre": "kill-port 3000",
    "dev:app": "nuxt dev",
    "lint:js": "eslint --ext \".cjs,.mjs,.ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "format:js": "prettier --write \"**/*.{cjs,mjs,ts,vue,json}\"",
    "format:css": "prettier --write \"**/*.{css,scss}\"",
    "test:ts": "nuxt typecheck",
    "test:unit": "vitest run",
    "test:unit:ui": "vitest --ui",
    "test:unit:dev": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report tests/e2e/report",
    "lint": "run-s lint:*",
    "format": "run-s format:*",
    "test": "run-s test:*",
    "dev": "run-s dev:pre dev:app",
    "build": "run-s build:pre build:app",
    "preview": "BUILD_TYPE=preview run-s build && nuxt preview"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@iconify-json/mdi": "^1.1.64",
    "@nuxt/test-utils": "^3.9.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.11.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@unocss/nuxt": "^0.58.3",
    "@unocss/preset-icons": "^0.58.3",
    "@vercel/analytics": "^1.1.2",
    "@vercel/speed-insights": "^1.0.8",
    "@vitest/ui": "^1.2.0",
    "@vue/test-utils": "^2.4.3",
    "@vueuse/core": "^10.7.1",
    "@vueuse/nuxt": "^10.7.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "happy-dom": "^13.1.4",
    "husky": "^8.0.0",
    "kill-port-process": "^3.2.1",
    "lint-staged": "^15.2.0",
    "msw": "^2.1.4",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.9.0",
    "playwright-core": "^1.40.1",
    "playwright-msw": "^3.0.1",
    "prettier": "^3.1.1",
    "sass": "^1.70.0",
    "stylelint": "^16.2.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unocss-preset-animations": "^1.0.1",
    "unocss-preset-shadcn": "^0.2.3",
    "vitest": "^1.2.0",
    "vue": "^3.4.5",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "@unocss/reset": "^0.58.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-vue-next": "^0.316.0",
    "pinia": "^2.1.7",
    "radix-vue": "^1.3.2",
    "tailwind-merge": "^2.2.1"
  }
}
