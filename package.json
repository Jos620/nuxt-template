{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "build:pre": "rimraf .output",
    "build:app": "nuxt build",
    "dev:pre": "kill-port 3000",
    "dev:app": "nuxt dev",
    "lint:js": "eslint --ext \".cjs,.mjs,.ts,.js,.vue\" --ignore-path .gitignore .",
    "format:js": "prettier --write \"**/*.{cjs,mjs,ts,vue,json}\"",
    "test:ts": "nuxt typecheck",
    "run:lint": "run-s format:js lint:js",
    "run:test": "run-s test:ts",
    "run:dev": "run-s dev:pre dev:app",
    "run:build": "run-s build:pre build:app"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@unocss/nuxt": "^0.58.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "kill-port-process": "^3.2.1",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.9.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "vue": "^3.4.5",
    "vue-router": "^4.2.5",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "@unocss/reset": "^0.58.3",
    "pinia": "^2.1.7"
  }
}
